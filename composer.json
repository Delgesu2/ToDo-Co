{
    "name": "saro0h/to-do-list",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "symfony/symfony": "3.4.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.10.2",
        "doctrine/doctrine-cache-bundle": "^1.3.5",
        "symfony/swiftmailer-bundle": "^3.2.0",
        "symfony/monolog-bundle": "^3.3.1",
        "symfony/polyfill-apcu": "^1.11.0",
        "sensio/distribution-bundle": "^5.0.24",
        "sensio/framework-extra-bundle": "^5.2.4",
        "incenteev/composer-parameter-handler": "^2.1.3",
        "swiftmailer/swiftmailer": "^6.2",
        "squizlabs/php_codesniffer": "^3.4",
        "snc/redis-bundle": "2.1.x-dev",
        "predis/predis": "1.1.1",
        "phpunit/phpunit": "^8.1",
        "phpmetrics/phpmetrics": "^2.4"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.1.7",
        "symfony/phpunit-bridge": "^4.2.4",
        "symfony/web-server-bundle": "^4.2.4",
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "symfony/browser-kit": "^4.2",
        "symfony/css-selector": "^4.2"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
